{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "extends": [
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "valid-jsdoc": 1,
    "operator-linebreak": [1, "before"],
    "yoda": [1, "never", { "exceptRange": false, "onlyEquality": true }],
    "no-undef": 0,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-shadow-restricted-names": 1,
    "no-redeclare": 2,
    "no-self-assign": 2,
    "no-delete-var": 2,
    "no-label-var": 1,
    "no-lone-blocks": 1,
    "no-undefined": 1,
    "no-use-before-define": 0,
    "comma-spacing": [1, { "before": false, "after": true }],
    "key-spacing": [1, { "afterColon": true }],
    "no-nested-ternary": 1,
    "comma-dangle": [1, "never"],
    "comma-style": [1, "last"],
    "no-constant-condition": [1, { "checkLoops": false }],
    "no-debugger": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-empty": 1,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 1,
    "no-extra-boolean-cast": 1,
    "no-fallthrough": 0,
    "no-extra-semi": 0,
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-implied-eval": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-proto": 1,
    "no-obj-calls": 2,
    "no-regex-spaces": 1,
    "no-unsafe-finally": 1,
    "no-unreachable": 1,
    "use-isnan": 2,
    "valid-typeof": 2,
    "wrap-iife": 1,
    "no-unexpected-multiline": 2,
    "no-multiple-empty-lines": 1,
    "dot-notation": [1, { "allowKeywords": true }],
    "eqeqeq": [1, "allow-null"],
    "no-caller": 2,
    "no-else-return": 1,
    "no-loop-func": 1,
    "no-octal": 2,
    "no-throw-literal": 1,
    "no-unused-vars": 1,
    "no-new-require": 2,
    "no-path-concat": 1,
    "brace-style": [1, "1tbs", {}],
    "computed-property-spacing": [1, "never"],
    "eol-last": 1,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-unneeded-ternary": 1,
    "operator-assignment": [1, "always"],
    "semi-spacing": [1, { "after": true }],
    "semi": [1, "never"],
    "space-before-blocks": [1, "always"],
    "space-before-function-paren": [1, "always"],
    "arrow-parens": [1, "as-needed"],
    "arrow-spacing": [1, { "before": true, "after": true }],
    "constructor-super": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "object-shorthand": [1, "always"],
    "prefer-spread": 1,
    "no-inner-declarations": 1,
    "no-iterator": 2,
    "no-new-func": 2,
    "no-new-symbol": 2,
    "no-tabs": 1,
    "no-multi-spaces": 1,
    "no-trailing-spaces": 1,
    "no-this-before-super": 2,
    "no-unsafe-negation": 2,
    "no-useless-call": 1,
    "no-useless-computed-key": 1,
    "no-useless-constructor": 1,
    "no-useless-escape": 1,
    "no-useless-rename": 1,
    "no-with": 2,
    "no-array-constructor": 2,
    "accessor-pairs": 2,
    "object-property-newline": [1, { "allowMultiplePropertiesPerLine": true }],
    "rest-spread-spacing": 1,
    "space-in-parens": 1,
    "space-unary-ops": 1,
    "template-curly-spacing": 1,
    "new-parens": 1,
    "dot-location": [1, "property"],
    "func-call-spacing": 1,
    "block-spacing": 1,
    "camelcase": [1, { "properties": "never" }],
    "one-var": [1, "never"],
    "handle-callback-err": 2,
    "keyword-spacing": 1,
    "quotes": [1, "single"],
    "prefer-const": [1, { "destructuring": "all" }],
    "curly": [1, "multi-or-nest", "consistent"],
    "new-cap": [1, { "newIsCap": false, "capIsNew": false, "properties": false }],
    "indent": [1, 2, {
      "SwitchCase": 1,
      "VariableDeclarator": 1,
      "outerIIFEBody": 1,
      "MemberExpression": 1,
      "FunctionDeclaration": { "parameters": 1, "body": 1 },
      "FunctionExpression": { "parameters": 1, "body": 1 },
      "CallExpression": { "arguments": 1 },
      "ArrayExpression": 1,
      "ObjectExpression": 1,
      "ImportDeclaration": 1,
      "flatTernaryExpressions": false,
      "ignoreComments": true
    }]
  }
}
